	<!-- note: @top-level added in mode=" update_xml_step_move_pagebreak" -->
	<xsl:template match="*[local-name() = 'sections']/*[local-name() = 'page_sequence']/*[not(@top-level)]" priority="2">
		<xsl:choose>
